{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "appId",
    "event",
    "session",
    "appVer",
    "network",
    "visitor",
    "schemaName",
    "stm"
  ],
  "properties": {
    "appId": {
      "$id": "#/properties/appId",
      "type": "string",
      "title": "The Appid Schema",
      "default": "",
      "examples": [
        "Trippi.vn"
      ],
      "pattern": "^(.*)$"
    },
    "event": {
      "$id": "#/properties/event",
      "type": "object",
      "title": "The Event Schema",
      "required": [
        "id",
        "createdAt",
        "eventType",
        "eventName",
        "notificationID",
        "notificationName",
        "notificationContent",
        "openTime"
      ],
      "properties": {
        "id": {
          "$id": "#/properties/event/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "f6d15d7d-b68f-41b1-8e4f-4bd622de37c0"
          ],
          "pattern": "^(.*)$"
        },
        "createdAt": {
          "$id": "#/properties/event/properties/createdAt",
          "type": "integer",
          "title": "The Createdat Schema",
          "default": 0,
          "examples": [
            1559549791
          ]
        },
        "eventType": {
          "$id": "#/properties/event/properties/eventType",
          "type": "string",
          "title": "The Eventtype Schema",
          "default": "",
          "examples": [
            "PushNotification"
          ],
          "pattern": "^(.*)$"
        },
        "eventName": {
          "$id": "#/properties/event/properties/eventName",
          "type": "string",
          "title": "The Eventname Schema",
          "default": "",
          "examples": [
            "pushNotification"
          ],
          "pattern": "^(.*)$"
        },
        "notificationID": {
          "$id": "#/properties/event/properties/notificationID",
          "type": "string",
          "title": "The Notificationid Schema",
          "default": "",
          "examples": [
            "abc"
          ],
          "pattern": "^(.*)$"
        },
        "notificationName": {
          "$id": "#/properties/event/properties/notificationName",
          "type": "string",
          "title": "The Notificationname Schema",
          "default": "",
          "examples": [
            "Trippi Voucher"
          ],
          "pattern": "^(.*)$"
        },
        "notificationContent": {
          "$id": "#/properties/event/properties/notificationContent",
          "type": "string",
          "title": "The Notificationcontent Schema",
          "default": "",
          "examples": [
            "abc abc abc abc abc abc abc abc abc"
          ],
          "pattern": "^(.*)$"
        },
        "openTime": {
          "$id": "#/properties/event/properties/openTime",
          "type": "integer",
          "title": "The Opentime Schema",
          "default": 0,
          "examples": [
            1559549791
          ]
        }
      }
    },
    "session": {
      "$id": "#/properties/session",
      "type": "object",
      "title": "The Session Schema",
      "required": [
        "createdAt",
        "id",
        "lastActiveAt",
        "offset",
        "platform",
        "osVer",
        "branchName",
        "modelName",
        "res",
        "tz",
        "manufacturerName"
      ],
      "properties": {
        "createdAt": {
          "$id": "#/properties/session/properties/createdAt",
          "type": "integer",
          "title": "The Createdat Schema",
          "default": 0,
          "examples": [
            1559542062
          ]
        },
        "id": {
          "$id": "#/properties/session/properties/id",
          "type": "string",
          "title": "The Id Schema",
          "default": "",
          "examples": [
            "ec39f099-9877-45d8-ad8e-198d675deb02"
          ],
          "pattern": "^(.*)$"
        },
        "lastActiveAt": {
          "$id": "#/properties/session/properties/lastActiveAt",
          "type": "integer",
          "title": "The Lastactiveat Schema",
          "default": 0,
          "examples": [
            1559549791
          ]
        },
        "offset": {
          "$id": "#/properties/session/properties/offset",
          "type": "string",
          "title": "The Offset Schema",
          "default": "",
          "examples": [
            "+07:00"
          ],
          "pattern": "^(.*)$"
        },
        "platform": {
          "$id": "#/properties/session/properties/platform",
          "type": "string",
          "title": "The Platform Schema",
          "default": "",
          "examples": [
            "Android"
          ],
          "pattern": "^(.*)$"
        },
        "osVer": {
          "$id": "#/properties/session/properties/osVer",
          "type": "string",
          "title": "The Osver Schema",
          "default": "",
          "examples": [
            "Android 8.1"
          ],
          "pattern": "^(.*)$"
        },
        "branchName": {
          "$id": "#/properties/session/properties/branchName",
          "type": "string",
          "title": "The Branchname Schema",
          "default": "",
          "examples": [
            "Samsung"
          ],
          "pattern": "^(.*)$"
        },
        "modelName": {
          "$id": "#/properties/session/properties/modelName",
          "type": "string",
          "title": "The Modelname Schema",
          "default": "",
          "examples": [
            "Samsung S8"
          ],
          "pattern": "^(.*)$"
        },
        "res": {
          "$id": "#/properties/session/properties/res",
          "type": "string",
          "title": "The Res Schema",
          "default": "",
          "examples": [
            "2960x1440"
          ],
          "pattern": "^(.*)$"
        },
        "tz": {
          "$id": "#/properties/session/properties/tz",
          "type": "string",
          "title": "The Tz Schema",
          "default": "",
          "examples": [
            "Asia/Bangkok"
          ],
          "pattern": "^(.*)$"
        },
        "manufacturerName": {
          "$id": "#/properties/session/properties/manufacturerName",
          "type": "string",
          "title": "The manufacturername Schema",
          "default": "",
          "examples": [
            "Google"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "appVer": {
      "$id": "#/properties/appVer",
      "type": "string",
      "title": "The Appver Schema",
      "default": "",
      "examples": [
        "1.0.0"
      ],
      "pattern": "^(.*)$"
    },
    "network": {
      "$id": "#/properties/network",
      "type": "object",
      "title": "The Network Schema",
      "required": [
        "phoneNumber",
        "carrierName",
        "networkType"
      ],
      "properties": {
        "phoneNumber": {
          "$id": "#/properties/network/properties/phoneNumber",
          "type": "string",
          "title": "The Phonenumber Schema",
          "default": "",
          "examples": [
            "0981349669"
          ],
          "pattern": "^(.*)$"
        },
        "carrierName": {
          "$id": "#/properties/network/properties/carrierName",
          "type": "string",
          "title": "The Carriername Schema",
          "default": "",
          "examples": [
            "Viettel"
          ],
          "pattern": "^(.*)$"
        },
        "networkType": {
          "$id": "#/properties/network/properties/networkType",
          "type": "string",
          "title": "The Networktype Schema",
          "default": "",
          "examples": [
            "Wifi"
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "visitor": {
      "$id": "#/properties/visitor",
      "type": "object",
      "title": "The Visitor Schema",
      "required": [
        "clientId",
        "userId"
      ],
      "properties": {
        "clientId": {
          "$id": "#/properties/visitor/properties/clientId",
          "type": "string",
          "title": "The Clientid Schema",
          "default": "",
          "examples": [
            "754047c4273395119bda4e5280d190d4"
          ],
          "pattern": "^(.*)$"
        },
        "userId": {
          "$id": "#/properties/visitor/properties/userId",
          "type": ["string", "null"],
          "title": "The Userid Schema",
          "default": "",
          "examples": [
            ""
          ],
          "pattern": "^(.*)$"
        }
      }
    },
    "schemaName": {
      "$id": "#/properties/schemaName",
      "type": "string",
      "title": "The Schemaname Schema",
      "default": "",
      "examples": [
        "mobile.pushNotification"
      ],
      "pattern": "^(.*)$"
    },
    "stm": {
      "$id": "#/properties/stm",
      "type": "integer",
      "title": "The Stm Schema",
      "default": 0,
      "examples": [
        1559549791
      ]
    }
  }
}